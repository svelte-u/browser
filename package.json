{
    "name": "@sveu/browser",
    "version": "0.20.0",
    "packageManager": "pnpm@8.4.0",
    "description": "A collection of browser utilities for Svelte",
    "author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
    "license": "MIT",
    "homepage": "https://github.com/svelte-u/browser#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/svelte-u/browser.git"
    },
    "bugs": {
        "url": "https://github.com/svelte-u/browser/issues"
    },
    "keywords": [
        "svelte",
        "browser",
        "webapi",
        "web-api"
    ],
    "type": "module",
    "scripts": {
        "build": "pnpm run clean && tsx scripts/build.ts && tsup",
        "clean": "tsx scripts/clean.ts",
        "git:add": "pnpm run build && pnpm run clean && git add .",
        "commit": "cz commit",
        "lint": "eslint .",
        "lint:fix": "pnpm run lint --fix",
        "release": "bumpp",
        "test": "vitest run",
        "test:watch": "vitest --watch"
    },
    "exports": {
        ".": {
            "import": "./index.js"
        }
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.2.0",
        "@testing-library/svelte": "^3.2.2",
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.1.0",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "bumpp": "^9.1.0",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "fast-glob": "^3.2.12",
        "fs-extra": "^11.1.1",
        "jsdom": "^21.1.1",
        "prettier": "^2.8.8",
        "svelte": "^3.59.0",
        "tsup": "^6.7.0",
        "tsx": "^3.12.7",
        "typescript": "^5.0.4",
        "vitest": "^0.31.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@sveu/shared": "^0.10.1"
    }
}
