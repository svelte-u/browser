{
	"semi": false,
	"singleQuote": false,
	"printWidth": 80,
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"trailingComma": "es5",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^svelte",
		"^@",
		"^\\$",
		"<THIRD_PARTY_MODULES>",
		"^[./]",
		"<TYPE_IMPORTS>"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true
}
